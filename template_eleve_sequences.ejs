<% 
	if(data.ressources.length > 0){

		index = 0;
				
		while(index < data.ressources.length) {

			if(data.ressources[index].urlThumbnail!="undefined")
			{
				var typePath = UrlThumbnailType.sequence + data.ressources[index].urlThumbnail;
			}else{
				var typePath = noUrlThumbnailType.sequence;
			}
%>
			<div class="accordion" id="accordion_<%=data.ressources[index].id%>">
			  	<div class="accordion-group">
					<div class="accordion-heading">
						<a class="accordion-toggle acc_sequence thumbnail" style="background: white;" data-toggle="collapse" data-parent="#accordion_<%=data.ressources[index].id%>" href="#collapse_<%=data.ressources[index].id%>">
							<ul class="inline">
								<li class="img_sequence">
									<img src="<%=typePath%>" border="0" alt="blank image">
								</li>
								<li class="info_sequence">
									<div class="titre"><%=unescape(data.ressources[index].title)%></div>
									<div class="desc"><%=unescape(data.ressources[index].description)%></div>
								</li>
							</ul>
						</a>
					</div>
					<div id="collapse_<%=data.ressources[index].id%>" class="accordion-body collapse">
						<!-- <div id="DetailCarousel_<%=data.ressources[index].id%>" class="carousel slide"> -->
							<div class="NavDetailMenuInterne" style="padding:0px 5px 5px 5px; margin-bottom: 10px;">
								<div class="arrow"></div>
								<div id="seqDetailCarousel_<%=data.ressources[index].id%>" class="carousel-inner eleve_sequence_content" data-resid="<%=data.ressources[index].id%>" >
								</div>
							</div>
						<!-- </div> -->	
					</div>
				</div>
			</div>
<%
			index++;
		}
	} else {
%>
		<div style="padding: 8px; color: black;">
			Aucun travaux Ã  faire.
		</div>
<%	
	}
%>